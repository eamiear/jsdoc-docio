{
  "tags": {
    "allowUnknownTags": true
  },
  "opts": {
    "private": true
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": true
    },
    "systemName"      : "Docio",
    "footer"          : "",
    "copyright"       : "Copyright Â© 2017 <a href='#' target='_self'>docio.</a>",
    "navType"         : "vertical",
    "theme"           : "docio",
    "includeDate"     : false,
    "linenums"        : true,
    "inverseNav"      : true
  }
}
